<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              android:id="@+id/lnarLayoutCdstrOS_PrincipalLayout"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
    <!--iniciando o layout-->
    <!--titulo-->
    <!--Lzyout topo-->
    <include layout="@layout/linearlayout_cadastraros_topo"
             android:layout_height="wrap_content"
             android:layout_width="match_parent"
             android:layout_gravity="top"/>
    <!--Layout Center-->
    <include layout="@layout/tablelayout_cadastraros_center"
             android:layout_height="80dp"
             android:layout_width="match_parent"
             android:layout_gravity="center"/>
    <!--layout bottom-->
    <include layout="@layout/tablelayout_cadastraros_bottom"
             android:layout_height="80dp"
             android:layout_width="match_parent"
             android:layout_gravity="center"/>
    <!--BotÃ£o salvar e localizar-->
    <include layout="@layout/linearlayout_cadastraros_btnsalvarelocalizar"
             android:layout_height="80dp"
             android:layout_width="match_parent"
             android:layout_gravity="center"/>
</LinearLayout>